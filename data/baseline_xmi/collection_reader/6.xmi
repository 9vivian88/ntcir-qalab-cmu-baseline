ected="false" refList="214 222"/><types:AnswerChoice xmi:id="237" sofa="7" begin="1930" end="1946" id="(2)" text="(2)(i)-(iii)" isCorrect="false" isSelected="false" refList="251 259"/><types:AnswerChoice xmi:id="274" sofa="7" begin="1951" end="1966" id="(3)" text="(3)(i)-(iv)" isCorrect="false" isSelected="false" refList="288 296"/><types:AnswerChoice xmi:id="311" sofa="7" begin="1971" end="1988" id="(4)" text="(4)(ii)-(iii)" isCorrect="false" isSelected="false" refList="325 333"/><types:AnswerChoice xmi:id="348" sofa="7" begin="1993" end="2009" id="(5)" text="(5)(ii)-(iv)" isCorrect="false" isSelected="false" refList="362 370"/><types:AnswerChoice xmi:id="385" sofa="7" begin="2014" end="2029" id="⑥" text="⑥(iii)-(iv)" isCorrect="false" isSelected="false" refList="399 407"/><types:AnswerChoice xmi:id="463" sofa="7" begin="2194" end="2294" id="(1)" text="(1)Thales and others advocated natural philosophy and sought to identify the properties of nature." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="478" sofa="7" begin="2299" end="2369" id="(2)" text="(2)The Sophists emerged and claimed the existence of absolute truth." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="493" sofa="7" begin="2374" end="2443" id="(3)" text="(3)Plato advocated the Theory of Forms and the Theory of the State." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="508" sofa="7" begin="2448" end="2519" id="(4)" text="(4)Aristotle codified various academic studies and systematized them." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="634" sofa="7" begin="3195" end="3235" id="(1)" text="(1)a-(ii)b-(i)c-(iv)d-(iii)" isCorrect="false" isSelected="false" refList="648 656 664 672 680 688 696 704"/><types:AnswerChoice xmi:id="737" sofa="7" begin="3240" end="3280" id="(2)" text="(2)a-(ii)b-(iv)c-(i)d-(iii)" isCorrect="false" isSelected="false" refList="751 759 767 775 783 791 799 807"/><types:AnswerChoice xmi:id="840" sofa="7" begin="3285" end="3325" id="(3)" text="(3)a-(iii)b-(iv)c-(i)d-(ii)" isCorrect="false" isSelected="false" refList="854 862 870 878 886 894 902 910"/><types:AnswerChoice xmi:id="943" sofa="7" begin="3330" end="3370" id="(4)" text="(4)a-(iii)b-(i)c-(iv)d-(ii)" isCorrect="false" isSelected="false" refList="957 965 973 981 989 997 1005 1013"/><types:AnswerChoice xmi:id="1152" sofa="7" begin="4837" end="4941" id="(1)" text="(1)Qin Shihuangdi, the First Emperor attacked the Tibetan Empire and repaired the Great Wall of China." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1167" sofa="7" begin="4946" end="5063" id="(2)" text="(2)Emperor Wu of the Western Han dynasty overthrew Wiman Joseon and established the Lelang Commandery in his place." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1182" sofa="7" begin="5068" end="5167" id="(3)" text="(3)The incident in which a Ming Emperor was held captive by Altan Khan is called the Tumu Crisis." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1197" sofa="7" begin="5172" end="5250" id="(4)" text="(4)Kublai Khan of the Yuan destroyed the Taungoo Dynasty of Burma (Myanmar)." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1262" sofa="7" begin="5441" end="5527" id="(1)" text="(1)After the establishment of the dynasty, it designated Kaesong as the new capital." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1277" sofa="7" begin="5532" end="5649" id="(2)" text="(2)It attached importance to the Cheng-Zhu school and designated this as the state-sanctioned approach to learning." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1292" sofa="7" begin="5654" end="5717" id="(3)" text="(3)It instituted the phonetic alphabet called Hunminjeongeum." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1307" sofa="7" begin="5722" end="5798" id="(4)" text="(4)The people of the ruling class in this dynasty were called the yangban." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1372" sofa="7" begin="5974" end="6096" id="(1)" text="(1)The Kunyu Wanguo Quantu (Map of the Myriad Countries of the World) was compiled at the order of the Qianlong Emperor." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1387" sofa="7" begin="6101" end="6164" id="(2)" text="(2)Gu Yanwu criticized kaozheng xue (evidential scholarship)." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1402" sofa="7" begin="6169" end="6249" id="(3)" text="(3)The Literary Inquisition took place and anti-Qing sentiment was suppressed." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1417" sofa="7" begin="6254" end="6298" id="(4)" text="(4)Qian Daxin devised the Manchu alphabet." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1482" sofa="7" begin="6476" end="6587" id="(1)" text="(1)The influence of the Khwarazmian dynasty extended as far as Central Asia, but it was overthrown by Ghazan." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1497" sofa="7" begin="6592" end="6684" id="(2)" text="(2)Je Tsongkhapa reformed Tibetan Buddhism and founded the Gelug school (Yellow Hat Sect)." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1512" sofa="7" begin="6689" end="6807" id="(3)" text="(3)The Zunghar Khanate engaged in ongoing opposition to the Qing dynasty, but it weakened and was annexed by Russia." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1527" sofa="7" begin="6812" end="6934" id="(4)" text="(4)The Qing dynasty established the zhong shu sheng (Central Secretariat) to control regions such as Mongolia and Tibet." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1621" sofa="7" begin="8024" end="8133" id="(1)" text="(1)After Napoleon I was ousted, Louis XVI was restored to the throne, thereby reviving the Bourbon dynasty." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1636" sofa="7" begin="8138" end="8267" id="(2)" text="(2)As a result of the July Revolution, a system of defending the profits of bankers and other major financiers was established." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1651" sofa="7" begin="8272" end="8365" id="(3)" text="(3)Louis-Napoléon was elected in the presidential election held during the Second Republic." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1666" sofa="7" begin="8370" end="8532" id="(4)" text="(4)The common people of Paris, who were resentful of the humiliating terms of the peace agreement with Germany, declared the establishment of the Paris Commune." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1731" sofa="7" begin="8777" end="8791" id="(1)" text="(1)Symbolism" isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1746" sofa="7" begin="8796" end="8811" id="(2)" text="(2)Classicism" isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1761" sofa="7" begin="8816" end="8832" id="(3)" text="(3)Romanticism" isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1776" sofa="7" begin="8837" end="8849" id="(4)" text="(4)Realism" isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1841" sofa="7" begin="9045" end="9137" id="(1)" text="(1)It forced Joseon to embark on a process of opening up during the rule of the Daewongun." isCorrect="true" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1856" sofa="7" begin="9142" end="9237" id="(2)" text="(2)It came into conflict with Spain about dividing up Africa and caused the Fashoda Incident." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1871" sofa="7" begin="9242" end="9324" id="(3)" text="(3)It concluded a friendship treaty with Japan before the USA and the UK did so." isCorrect="false" isSelected="false" refList=""/><types:AnswerChoice xmi:id="1886" sofa="7" begin="9329" end="9392" id="(4)" text="(4)It dispatched troops to Mexico, but this ended in failure." isCorrect="false" isSelected="false" refList=""/><types:ListItem xmi:id="150" sofa="7" begin="1540" end="1664" label="(i)" text="Francis Bacon advocated empiricism (inductive reasoning), which seeks to elicit general principles from factual observation." id="L1"/><types:ListItem xmi:id="157" sofa="7" begin="1674" end="1779" label="(ii)" text="Descartes established rationalism (deductive reasoning), which seeks to investigate truth mathematically." id="L2"/><types:ListItem xmi:id="164" sofa="7" begin="1790" end="1831" label="(iii)" text="Spinoza rejected continental rationalism." id="L3"/><types:ListItem xmi:id="171" sofa="7" begin="1841" end="1903" label="(iv)" text="British empiricism began with Hume and was perfected by Locke." id="L4"/><types:ListItem xmi:id="584" sofa="7" begin="2687" end="2814" label="(i)" text="He advocated the existence of universal truth and thought of the unity of knowledge and virtue as the way of life for citizens." id="L5"/><types:ListItem xmi:id="591" sofa="7" begin="2824" end="2948" label="(ii)" text="He thought that if a person took the right path, they could free themself from the suffering of birth, age, sickness, death." id="L6"/><types:ListItem xmi:id="598" sofa="7" begin="2959" end="3074" label="(iii)" text="He attached importance to the practice of family morality and considered it to be the foundations for social order." id="L7"/><types:ListItem xmi:id="605" sofa="7" begin="3084" end="3188" label="(iv)" text="He engaged in a fundamental reflection on human cognitive abilities and established critical philosophy." id="L8"/><types:QData xmi:id="178" sofa="7" begin="1532" end="1904" text="(i)Francis Bacon advocated empiricism (inductive reasoning), which seeks to elicit general principles from factual observation.&#10;                                    (ii)Descartes established rationalism (deductive reasoning), which seeks to investigate truth mathematically.&#10;                                    (iii)Spinoza rejected continental rationalism.&#10;                                    (iv)British empiricism began with Hume and was perfected by Locke." id="D18" listItems="150 157 164 171"/><types:QData xmi:id="612" sofa="7" begin="2679" end="3189" text="(i)He advocated the existence of universal truth and thought of the unity of knowledge and virtue as the way of life for citizens.&#10;                                    (ii)He thought that if a person took the right path, they could free themself from the suffering of birth, age, sickness, death.&#10;                                    (iii)He attached importance to the practice of family morality and considered it to be the foundations for social order.&#10;                                    (iv)He engaged in a fundamental reflection on human cognitive abilities and established critical philosophy." id="D19" listItems="584 591 598 605"/><types:Underlined xmi:id="1077" sofa="7" begin="3407" end="3423" id="U22" label="(1)" text="Qianlong Emperor"/><types:Underlined xmi:id="1100" sofa="7" begin="3849" end="3931" id="U23" label="(2)" text="Joseon intellectual Pak Jiwon described it in his work Yeolha Ilki (Rehe Diaries)."/><types:Underlined xmi:id="1107" sofa="7" begin="4201" end="4245" id="U24" label="(3)" text="learning and culture during the Qing dynasty"/><types:Underlined xmi:id="1114" sofa="7" begin="4417" end="4437" id="U25" label="(4)" text="inland ethnic groups"/><types:Underlined xmi:id="1582" sofa="7" begin="6946" end="7037" id="U26" label="(5)" text="The 19th century French literary giant Victor Hugo lived a tumultuous life as a politician."/><types:Underlined xmi:id="1589" sofa="7" begin="7735" end="7820" id="U27" label="(6)" text="was also a time when France adopted a more aggressive posture toward other countries."/><types:TestDocument xmi:id="1967" sofa="7" begin="0" end="0" id="6" instruction="1" QAList="112 437 535 1061 1224 1334 1444 1554 1693 1803 1913"/><tcas:DocumentAnnotation xmi:id="1974" sofa="7" begin="0" end="9394" language="en"/><examples:SourceDocumentInformation xmi:id="1979" sofa="7" begin="0" end="0" uri="file:/home/diwang/Dropbox/oaqa-workspace/ntcir-qalab-cmu-baseline/./input/97-01-05-09/quest.xml" offsetInSource="6" documentSize="398134" lastSegment="true"/><types2:ExperimentMeta xmi:id="1987" ExperimentId="4f9c6670-b357-46c4-a3d9-a9c890173ac6" DatasetId="./input/97-01-05-09" ExperimentName="World History QA" ExperimentInvoker="diwang"/><types:Question xmi:id="1901" sofa="7" begin="0" end="0" id="Q2001_35" contextData="1574" setinstruction="1" qdataList="" questionType="sentence" knowledgeType="KS" instruction="1823"/><types:QuestionAnswerSet xmi:id="1913" sofa="7" begin="0" end="0" question="1901" answerChoiceList="1841 1856 1871 1886"/><types:Question xmi:id="1791" sofa="7" begin="0" end="0" id="Q2001_34" contextData="1574" setinstruction="1" qdataList="" questionType="term_other" knowledgeType="KS" instruction="1713"/><types:QuestionAnswerSet xmi:id="1803" sofa="7" begin="0" end="0" question="1791" answerChoiceList="1731 1746 1761 1776"/><types:Question xmi:id="1681" sofa="7" begin="0" end="0" id="Q2001_33" contextData="1574" setinstruction="1" qdataList="" questionType="sentence" knowledgeType="KS" instruction="1603"/><types:QuestionAnswerSet xmi:id="1693" sofa="7" begin="0" end="0" question="1681" answerChoiceList="1621 1636 1651 1666"/><types:Question xmi:id="1542" sofa="7" begin="0" end="0" id="Q2001_32" contextData="1069" setinstruction="1" qdataList="" questionType="sentence" knowledgeType="KS" instruction="1464"/><types:QuestionAnswerSet xmi:id="1554" sofa="7" begin="0" end="0" question="1542" answerChoi